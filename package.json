{
  "name": "final-project-backend",
  "version": "1.0.0",
  "description": "This repository contains the backend codebase for the final project. It is built using Node.js and Express.js, with a layered architecture for better code organization and scalability. The backend is responsible for handling business logic, managing the database, and providing APIs for the frontend.",
  "main": "./src/index.js",
  "type": "module",
  "scripts": {
    "start": "cross-env NODE_ENV=production node ./src/index.js",
    "dev": "cross-env NODE_ENV=development nodemon ./src/index.js",
    "test": "jest --coverage",
    "generate-openapi": "dotenv -e .env.local -- node ./openapi.js",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "db:reset": "npx prisma migrate reset --preview-feature",
    "db:migrate": "npx prisma migrate dev --preview-feature",
    "db:studio": "npx prisma studio",
    "db:seed": "npx prisma db seed"
  },
  "prisma": {
    "seed": "node ./prisma/seeder/main.seed.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "@sentry/node": "^8.38.0",
    "@sentry/profiling-node": "^8.37.1",
    "babel-jest": "^29.7.0",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.10",
    "express": "^4.21.1",
    "express-list-endpoints": "^7.1.1",
    "googleapis": "^144.0.0",
    "graphology": "^0.25.4",
    "hashids": "^2.3.0",
    "imagekit": "^5.2.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "midtrans-client": "^1.4.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-qr-image": "^0.0.1",
    "nodemailer": "^6.9.16",
    "otplib": "^12.0.1",
    "postman2openapi": "^1.2.1",
    "socket.io": "^4.8.1",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/eslint-parser": "^7.25.9",
    "@babel/preset-env": "^7.26.0",
    "@eslint/js": "^9.14.0",
    "dotenv-cli": "^7.4.2",
    "eslint": "^9.14.0",
    "globals": "^15.12.0",
    "http-mocks": "^1.3.2",
    "jest": "^29.7.0",
    "nodemon": "^3.1.7",
    "prisma": "^5.22.0",
    "supertest": "^7.0.0",
    "xlsx": "^0.18.5"
  },
  "jest": {
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    }
  }
}
